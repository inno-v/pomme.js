<script src="assets/build.js"></script>
<script>
(function() {
    var Pomme = require('pomme');
    
    var channel = new Pomme({
        'scope': '<%= @scope %>'
    });
    
    // By default you can eval code in this context.
    channel.on('eval', function(code) {
        eval(code);
    });
})();
</script>