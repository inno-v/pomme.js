<script src="assets/build.js"></script>
<script>
(function() {
    var Sam = require('samskipti');
    
    var chanAppsA = new Sam({
        'scope': '<%= @scope %>'
    });
    
    chanAppsA.on('reverse', function(obj) {
        return obj.text.split('').reverse().join('');
    });
})();
</script>